<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite use-query-string="true">
	<rule>
		<note>
			mock service
        </note>
		<from>^/mock/(\d*?)/(.*)$</from>
		<to type="redirect">/mock/createData.action?id=$1&amp;pattern=/${escape:utf8:$2}</to>
	</rule>
	<rule>
		<note>
			mock service
        </note>
		<from>^/mockjs/(\d*?)/(.*)$</from>
		<to type="redirect">/mock/createRule.action?id=$1&amp;pattern=/${escape:utf8:$2}</to>
	</rule>
</urlrewrite>