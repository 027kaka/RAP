<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
#parse('/tcom/template.rap.vm')
<html>
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
		<title>欢迎使用RAP        </title>
		#includeNewRapStatic
		<link rel="stylesheet" href="./group/index.css" type="text/css" media="screen">
	</head>
	<body>
	    #bodyNewStart
	    <div class="page">
		    <div class="groups-con content">
		    	<h2>Dashboard</h2>
		    	<div class="groups">
		    	</div>
		    </div>
	    </div>
	   
<script type="text/tmpl" id="group-tmpl">
{{#groups}}
<div class="group">
    <h3>
        <span class="group-name">{{{name}}}</span>
    </h3>
    <div class="box-con clearfix">
		{{#projects}}
        <div class="box" data-projid="{{id}}">
            <div class="info">
                <div class="title">{{name}}</div>
                <div class="intro">{{desc}}</div>
            </div>
            <div class="status">{{status}}</div>
			{{#important}}
				<div class="star"><i class="glyphicon glyphicon-star"></i></div>
			{{/important}}
        </div>
		{{/projects}}
    </div>
</div>
{{/groups}}
</script>
		</script>
	    <script src="./home/index.js"></script>
	    #bodyEnd
	</body>
</html>