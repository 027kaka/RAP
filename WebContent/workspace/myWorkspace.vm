<!DOCTYPE html>
#parse('/tcom/template.rap.vm')
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <title>$!utils.escapeInH($workspace.project.name) - Doc Edit        </title>
        <link rel="shortcut icon" href="${staticRoot}/img/favicon.ico">
        #includeNewRapStatic
        <link type="text/css" rel="stylesheet" href="${staticRoot}/css/defcontrol.css" />
        <link type="text/css" rel="stylesheet" href="myWorkspace.css?_r=$!timeStamp" />
        <script src="${staticRoot}/js/util/jsformat.js"></script>
    </head>
    <body class="workspace #if(!$accessable)read-only#end">
        #bodyNewStart(true)
        <div id="div-fixed-panel">
            <div class="clearfix">

                <div id="div-w-message" class="label" style="display:none;">
                </div>
                <div id="div-control-panel">
                    <div id="div-workspace-search">
                        <input type="text" id="inputSearch" class="form-control" placeholder="Workspace Search">
                        </input>
                        <span class="glyphicon glyphicon-search"></span>
                        <div class="dropdown clearfix" id="dropdown-workspace-search" style="display:none;">
                            <ul class="dropdown-menu" style="display:block" role="menu" aria-labelledby="dropdownMenu2">
                                <li role="presentation" class="dropdown-header">Dropdown header</li>
                                <li role="presentation"><a role="menuitem" tabindex="-1" href="#">Action</a></li>
                                <li role="presentation"><a role="menuitem" tabindex="-1" href="#">Another action</a></li>
                                <li role="presentation"><a role="menuitem" tabindex="-1" href="#">Something else here</a></li>
                                <li role="presentation" class="divider"></li>
                                <li role="presentation" class="dropdown-header">Dropdown header</li>
                                <li role="presentation"><a role="menuitem" tabindex="-1" href="#">Separated link</a></li>
                            </ul>
                        </div>
                    </div>
                    <button id="btnFastSave" type="button" class="btn btn-warning btn-sm" data-toggle="tooltip" data-placement="bottom" onclick="ws.quickSave(); return false;"  style="display: none" title="Save by one click">
                        <span class="glyphicon glyphicon-flash"></span>
                       Fast Save 
                    </button>
                    <button id="btnSave" type="button" class="btn btn-default btn-sm" data-toggle="tooltip" data-placement="bottom" onclick="ws.switchToViewMode(true); return false;"  style="display: none" title="Save with comments">
                        <span class="glyphicon glyphicon-floppy-disk"></span>
                        Save
                    </button>
                    <button id="btnCancel" type="button" class="btn btn-danger btn-sm" data-toggle="tooltip" data-placement="bottom" onclick="ws.switchToViewMode(false); return false;"  style="display: none" title="Cancel edit, abandon all unsaved change.">
                        <span class="glyphicon glyphicon-trash"></span>
                        Cancel
                    </button>
                    #if($accessable)
                    <button id="btnEdit" type="button" class="btn btn-primary btn-sm" data-toggle="tooltip" data-placement="bottom" onclick="ws.switchToEditMode(); return false;" title="Enter edit view and lock the project.">
                        <span class="glyphicon glyphicon-pencil"></span>
                       Edit 
                    </button>

                    <button id="btnVersion" type="button" class="btn btn-default btn-sm" data-toggle="tooltip" data-placement="bottom" onclick="ws.version(); return false;" title="Version control">
                        <span class="glyphicon glyphicon-refresh"></span>
                        Version
                    </button>
                    #end
                    <button id="btnExitVersion" type="button" class="btn btn-default btn-sm" data-toggle="tooltip" data-placement="bottom" onclick="ws.exitVersion(); return false;" style="display: none;" title="Exit version control">
                        <span class="glyphicon glyphicon-arrow-left"></span>
                        Exit
                    </button>

                    <button id="btnToggleMockDisplay" type="button" class="btn btn-default btn-sm" data-toggle="tooltip" data-placement="bottom" onclick="ws.toggleMockDisplay();" title="Show/Hide MockJS Infos">
                        <span class="glyphicon glyphicon-headphones"></span>
                        Mock
                    </button>
                    #if($accessable)
                    <button id="btnExport" type="button" class="btn btn-default btn-sm" data-toggle="tooltip" data-placement="bottom" onclick="ws.showPluginCode(); return false;" title="Config project router, abtain plugin code...">
                        <span class="glyphicon glyphicon-wrench"></span>
                       Config 
                    </button>
                    #end
                    <div class="btn-group">
                        <button type="button" class="btn btn-default btn-sm dropdown-toggle" data-toggle="dropdown">
                             <span class="glyphicon glyphicon-th-large"></span>
                            More <span class="caret"></span>
                        </button>
                        <ul class="dropdown-menu" role="menu">
                            <li>
                                <a href="#" onclick="window.open('$!workspaceUrl.export?projectId=$!projectId');return false;">
                                    <span class="glyphicon glyphicon-file"></span>&nbsp;Export Word
                                </a>
                            </li>
                            <li>
                                <a href="/doc/${projectId}" target="_blank">
                                <span class="glyphicon glyphicon-new-window"></span>
                                Online Docs
                                </a>
                            </li>
                            <!--
                            <li>
                                <a href="http://thx.github.io/RAP/resources/rap-shortcuts/"  target="_blank">
                                    <span class="glyphicon glyphicon-flash"></span>
                                    Shortcuts
                                </a>
                            </li>
                            -->
                            <li>
                                <a href="https://github.com/thx/RAP/wiki/user_manual_cn"  target="_blank">
                                    <span class="glyphicon glyphicon-list-alt"></span>
                                    Help/Tutorials
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>

            </div>
            <div id="div-debug"></div>
            <div id="div-mt-list" class="clearfix"></div>
            <div id="div-add-p" style="display: none"><a href="#" class="btn btn-info btn-xs" onclick="ws.addP(); return false;"><i class="glyphicon glyphicon-plus" style="margin-right: 5px"></i>Add Page</a></div>
        </div>
        <div id="div-w">
            <div id="div-m-list"> </div>
            <div id="div-w-status"></div>
        </div>
        #bodyEnd


        ## [todo] mode case
        #if (1 == 1)
            #startFloater("saveVSSFloater" "" 475 354)
            <form name="formSaveVSSFloater">
                <div id="div-saveVSS-floater-message" style="display:none;" class="label">
                </div>
                <div id="div-saveVSS-floater-content">
                    <table class="table">
                        <tr>
                            <td class="title">Add Version</td>
                            <td class="radio-list clearfix">
                                <input type="radio" id="radioVersion1" name="radioVersion"  value="1" onchange="ws.radioVersionChanged(1);" />
                                <label for="radioVersion1">1</label>
                                <input type="radio" id="radioVersion2" name="radioVersion"  value="2" onchange="ws.radioVersionChanged(2);" />
                                <label for="radioVersion2">2</label>
                                <input type="radio" id="radioVersion3" name="radioVersion"  value="3" onchange="ws.radioVersionChanged(3);" />
                                <label for="radioVersion3">3</label>
                                <input type="radio" id="radioVersion4" name="radioVersion"  value="4" onchange="ws.radioVersionChanged(4);" />
                                <label for="radioVersion4">4</label>
                            </td>
                        </tr>
                        <tr>
                            <td class="title">Preview Change:</td>
                            <td><div id="divVersion"></div></td>
                        </tr>
                        <tr>
                            <td class="title">Submit Comments:</td>
                            <td><textarea id="txtDescription" minLen="5" message="length > 5." style="width:320px; height: 150px"></textarea></td>
                        </tr>
                        <tr>
                            <td></td>
                            <td>
                                <button type="button" class="btn btn-primary" onclick="ws.doSaveVSS(); return false;">
                                    <span class="glyphicon glyphicon-ok"></span>
                                    Submit
                                </button>
                                <button type="button" class="btn btn-default" onclick="ws.cancelSaveVSS(); return false;">
                                    <span class="glyphicon glyphicon-remove"></span>
                                    Cancel
                                </button>
                            </td>
                        </tr>
                    </table>
                </div>
            </form>
            #endFloater
        #elseif ($workspace.modeInt == 2)
            #startFloater("checkinFloater" "Check In" 600 400)
            <div class="myfloater">
                <div id="div-checkin-floater-message" style="display:none;"></div>
                <div id="div-checkin-floater-content">
                </div>
                <div class="div-floater-control">
                    <input type="button" value="Close"  onclick="ws.closeCheckInPanel();" />
                </div>
            </div>
            #endFloater

            #startFloater("checkoutFloater" "Check Out" 600 400)
            <div class="myfloater">
                <div id="div-checkout-floater-message" style="display:none;">
                </div>
                <div id="div-checkout-floater-content">
                </div>
                <div class="div-floater-control">
                    <input type="button" value="Close"  onclick="ws.closeCheckOutPanel();" />
                </div>
            </div>
            #endFloater
            #startFloater("validateFloater" "Validating" 600 400)
            <div class="myfloater">
                <div class="warn">No length validator.</div>
            </div>
            #endFloater

            #startFloater("saveFloater" "Save/Load Workspace" 600 400)
            <div class="myfloater">
                <div id="div-save-floater-message" style="display:none;" class="label">
                </div>
                <div id="div-save-floater-content">
                </div>
                <div class="div-floater-control">
                    <input type="button" value="Load"  onclick="ws.querySave();"/>&nbsp;&nbsp;&nbsp;
                    <input type="button" value="Save"  onclick="ws.updateSave();"/>&nbsp;&nbsp;&nbsp;
                    <input type="button" value="Delete"  onclick="ws.removeSave();"/>&nbsp;&nbsp;&nbsp;
                    <input type="button" value="Cancel"  onclick="ws.closeSavePanel();"/>
                </div>
            </div>
            #endFloater
        #end

        #startFloater("versionFloater" "" 600 400)
        <div class="myfloater">
            <div id="div-version-floater-message" class="label" style="display:none;">
            </div>
            <div id="div-version-floater-content">
            </div>
            <div class="div-floater-control">
                <button id="btnObserveVersion" type="button" class="btn btn-primary" onclick="ws.readProjectOfVersion();" title="View specified version in read-only mode.">
                    <span class="glyphicon glyphicon-eye-open"></span>
                    View Version.
                </button>
                &nbsp;&nbsp;&nbsp;
                <button id="btnSwitchVersion" type="button" class="btn btn-danger" onclick="ws.switchVersion();" title="Switch to specified version">
                    <span class="glyphicon glyphicon-repeat"></span>
                    Switch Version
                </button>&nbsp;&nbsp;&nbsp;
                <button type="button"  class="btn btn-default" onclick="ws.closeVersionPanel();">
                    <span class="glyphicon glyphicon-remove">
                    </span>
                    Cancel
                </button>
            </div>
        </div>
        #endFloater

        #startFloater("editAFloater" "Edit Action" 600 300)
        <form name="formEditAFloater">
        <div id="floaterEditAFloater" class="myfloater">
            <input type="hidden" id="editAFloater-id" />
            <input type="hidden" id="editAFloater-pageId" />
            <table>
                <tr>
                    <td class="title">Name</td>
                    <td class="input">
                        <input type="text" id="editAFloater-name" minLen="1" message="Action Name can't be empty." maxlength="50" />
                    </td>
                </tr>
                <tr>
                    <td class="title">Action Type</td>
                    <td class="radio-list clearfix">
                        <input type="radio" id="editAFloater-type-1" name="editAFloater-type"  value="1"/>
                        <label for="editAFloater-type-1">GET</label>
                        <input type="radio" id="editAFloater-type-2" name="editAFloater-type"  value="2"/>
                        <label for="editAFloater-type-2">POST</label>
                        <input type="radio" id="editAFloater-type-3" name="editAFloater-type"  value="3"/>
                        <label for="editAFloater-type-3">PUT</label>
                        <input type="radio" id="editAFloater-type-4" name="editAFloater-type"  value="4"/>
                        <label for="editAFloater-type-4">DELETE</label>
                    </td>
                </tr>
                <tr>
                    <td class="title">Action URL</td>
                    <td class="input">
                        <input type="text" id="editAFloater-requestUrl" />
                    </td>
                </tr>
                <tr style="display:none;">
                    <td class="title">Related Tmpl</td>
                    <td class="input">
                        <input type="text" id="editAFloater-responseTemplate" />
                    </td>
                </tr>
                <tr>
                    <td class="title">DESC</td>
                    <td>
                        <textarea id="editAFloater-description" style="width: 450px; height: 150px; font-size: 12px; line-height: 1.5;" cols="20" rows="20"></textarea>
                    </td>
                </tr>
                <tr>
                    <td class="title">Response Data Format</td>
                    <td class="radio-list clearfix">
                        <input type="radio" id="editAFloater-struct-2" name="editAFloater-struct"  value="object"/>
                        <label for="editAFloater-struct-2">Object {}</label>
                        <input type="radio" id="editAFloater-struct-1" name="editAFloater-struct"  value="array"/>
                        <label for="editAFloater-struct-1">Array [{}, {}]</label>
                    </td>
                </tr>

                <tr>
                    <td colspan="2">

                    </td>
                </tr>
            </table>
            <div class="div-floater-control">
                <button type="button" class="btn btn-primary" onclick="ws.doAddOrEditA();">
                    <span class="glyphicon glyphicon-ok"></span>
                    Confirm
                </button>
                <button type="button" class="btn btn-default" onclick="ws.cancelEditA();">
                    <span class="glyphicon glyphicon-remove"></span>
                    Cancel
                </button>
            </div>
        </div>
        </form>
        #endFloater
        #startFloater("editPFloater" "Edit Page" 600 400)
        <form name="formEditPFloater">
            <div class="myfloater">
                <input type="hidden" id="editPFloater-id" />
                <input type="hidden" id="editPFloater-moduleId" />
                <table class="table">
                    <tr>
                        <td>Name</td>
                        <td>
                            <input type="text" id="editPFloater-name" minLen="1" message="Page name can't be empty." maxlength="50" style="width: 250px;" />
                        </td>
                    </tr>
                    <tr>
                        <td>DESC</td>
                        <td>
                            <textarea id="editPFloater-introduction" style="width: 450px; height: 200px; font-size: 12px; line-height: 1.5;" cols="20" rows="20"></textarea>
                        </td>
                    </tr>

                </table>
                <div class="div-floater-control">
                    <button type="button" class="btn btn-primary" onclick="ws.doAddOrEditP();">
                        <span class="glyphicon glyphicon-ok"></span>submit
                    </button>
                    <button type="button" class="btn btn-default" onclick="ws.cancelEditP();">
                        <span class="glyphicon glyphicon-remove"></span>cancel
                    </button>
                </div>
            </div>
        </form>
        #endFloater
        #startFloater("importJSONFloater" "JSON Import" 600 350)
        <form name="formImportJSONFloater">
            <div id="div-importJSON-floater-message" style="display:none;" class="label"></div>
            <div class="myfloater">
                <table class="table">
                    <tr>
                        <td>
                            <textarea placeholder="Please input JSON" id="importJSONFloater-text" style="width: 510px; height: 200px; font-size: 12px; line-height: 1.5;" cols="20" rows="20"></textarea>
                        </td>
                    </tr>
                </table>
                <div class="div-floater-control">
                    <button type="button" class="btn btn-primary" onclick="ws.doImportJSON();">
                        <span class="glyphicon glyphicon-ok"></span>
                        Import
                    </button>
                    <button type="button" class="btn btn-default" onclick="ws.cancelImportJSON();">
                        <span class="glyphicon glyphicon-remove"></span>
                        Cancel
                    </button>
                </div>
            </div>
        </form>
        #endFloater
        #startFloater("exportRAPDataFloater" "JSON Import" 600 350)
        <form name="formImportJSONFloater">
            <div class="myfloater">
                <table class="table">
                    <tr>
                        <td>
                            <textarea id="exportRAPDataFloater-text" style="width: 510px; height: 200px; font-size: 12px; line-height: 1.5;" cols="20" rows="20"></textarea>
                        </td>
                    </tr>
                </table>
                <div class="div-floater-control">
                    <input type="button" class="btn btn-default" onclick="ws.closeExportRAPDataFloater();"  value="Close" />
                </div>
            </div>
        </form>
        #endFloater
        #startFloater("pluginCodeFloater" "FE Toobox" 600 230)
        <form name="formPluginCodeFloater">
            <div class="myfloater">
                <div>Plugin Code</div>
                <div>
                    <input disabled="disabled" id="pluginCodeInput" style="width:550px;margin:8px 0;" type="text" value="&lt;script src=&quot;http://$!consts.DOMAIN_URL/rap.plugin.js?projectId=$!projectId&quot;&gt;&lt;/script&gt;">
                </div>
                <div>Project Router</div>
                <div>
                    <input id="divRelatedIds" style="width:550px;margin:8px 0;" type="text" value="$!workspace.project.relatedIds" placeholder="eg->24,32,56  project ids seperated by commas.">
                </div>
                <div class="div-floater-control">
                    <input type="button" class="btn btn-default" onclick="ws.closePluginCodeFloater(1);"  value="Save" />
                    <input type="button" class="btn btn-default" onclick="ws.closePluginCodeFloater();"  value="Cancel" />
                </div>
            </div>
            <div>

            </div>
        </form>
        #endFloater
        #startFloater("actionUrlFloater" "Action URL" 600 180)
        <form name="fformActionUrlFloater">
            <div class="myfloater">
                <div>
                    <input disabled="disabled" id="actionUrlFloater-input" style="width:550px;margin:8px 0;" type="text" value="">
                </div>
                <div class="div-floater-control">
                    <input type="button" class="btn btn-default" onclick="ws.closeActionUrlFloater();"  value="Close" />
                </div>
            </div>
        </form>
        #endFloater

        #startFloater("actionOpFloater" "Doc Operation" 600 230)
        <div class="myfloater">
            <form role="form">
                <div class="form-group">
                    <label >What operation will you perform on :&nbsp;<span id="actionOpFloater-title"></span>&nbsp;?</label>
                    <div>
                        <label class="radio-inline">
                            <input type="radio" id="actionOpFloater-op-move" name="actionOpFloater-op" value="move" checked="checked" />Move 
                        </label>
                        <label class="radio-inline">
                            <input type="radio" id="actionOpFloater-op-copy" name="actionOpFloater-op"  value="copy" /> Copy
                        </label>
                        <!--
                        <label class="radio-inline">
                            <input type="radio" id="actionOpFloater-op-mount" name="actionOpFloater-op"  value="mount" /> Mount
                        </label>
                        -->
                    </div>
                </div>
                <div class="form-group">
                    <label>Choose target</label>
                    <ul class="list-inline">
                        <li>
                            <select id="actionOpFloater-tab" onchange="ws.actionOpFloaterSelectChanged();" class="form-control"></select>
                        </li>
                        <li>
                            <select id="actionOpFloater-page" class="form-control"></select>
                        </li>
                    </ul>
                </div>
                <div class="form-group">
                    <input type="button" class="btn btn-success" onclick="ws.closeActionOpFloater(true);"  value="Confirm" />
                    <input type="button" class="btn btn-default" onclick="ws.closeActionOpFloater();"  value="Cancel" />
                </div>
            </form>
        </div>
        #endFloater
    </body>
    <script type="text/javascript">
        var urlObj = {
                "getModuleHtmlAjax" : "$workspaceUrl.getModuleHtmlAjax",
                "updateSave"        : "$workspaceUrl.updateSave",
                "updateCurrentSave" : "$workspaceUrl.updateCurrentSave",
                "querySave"         : "$workspaceUrl.querySave",
                "removeSave"        : "$workspaceUrl.removeSave",
                "checkIn"           : "$workspaceUrl.checkIn",
                "checkOut"          : "$workspaceUrl.checkOut",
                "imageRoot"         : "$staticRoot/img/",
                "lock"              : "$workspaceUrl.lock",
                "unlock"            : "$workspaceUrl.unlock",
                "ping"              : "$workspaceUrl.ping",
                "queryVersion"      : "$workspaceUrl.queryVersion",
                "switchVersion"     : "$workspaceUrl.switchVersion",
                "pageTester"        : "$testerUrl.pageTester",
                "updateReleatedIds" : "$projectUrl.updateReleatedIds",
                "myWorkspace"       : "http://$!consts.DOMAIN_URL$workspaceUrl.myWorkspace"
            },
            ws = rap.workspace,
            data = $workspaceJsonString,
            util = rap.util;

        if (data.projectData.moduleList.length === 0) {
            data.projectData.moduleList = [{"id":ws.generateId(),"name":"Some Tab","introduction":"","pageList":[{"moduleId":ws.generateId(),"name":"Some page","introduction":"","id":ws.generateId(),"isIdGenerated":true,"actionList":[{"pageId":ws.generateId(),"name":"Some action","requestType":"1","requestUrl":"","responseTemplate":"","description":"","id":ws.generateId(),"requestParameterList":[{"id":ws.generateId(),"identifier":"reqParam","name":"Some Param","remark":"","validator":"","dataType":"number","parameterList":[]}],"responseParameterList":[{"id":ws.generateId(),"identifier":"resParam","name":"Some Param","remark":"","validator":"","dataType":"number","parameterList":[]}]}]}]}];
        }

        ws.init(data, urlObj#if($actionId),$actionId#end, callbackHandler);

        function callbackHandler() {
            #if($mock == true)
                ws.toggleMockDisplay();
            #else
            if (window.location.toString().indexOf("mock=") == -1 && window.localStorage && window.localStorage.getItem("_isMockDisplay") === 'true') {
                ws.toggleMockDisplay();
            }
            #end
        }

        $('.btn').tooltip();

    </script>
</html>
